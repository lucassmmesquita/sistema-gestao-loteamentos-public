    "multer": "^1.4.5-lts.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
